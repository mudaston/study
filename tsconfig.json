{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "commonjs",
		"resolveJsonModule": true,
		"allowJs": false,
		"outDir": "build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	}
}
